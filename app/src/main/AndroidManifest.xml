<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lskj.ct.lifeatcar">

    <!--实现网络数据访问-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--网络变化权限-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--Read storage-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!--write storage-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <!--配置渠道-->
    <!--<meta-data-->
    <!--android:name="UMENG_CHANNEL"-->
    <!--android:value="${UMENG_CHANNEL_VALUE}" />-->

    <application
        android:name=".app.LifeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Base">
        <!--ali hoxfix start-->
        <meta-data
            android:name="com.taobao.android.hotfix.IDSECRET"
            android:value="24794169" />
        <meta-data
            android:name="com.taobao.android.hotfix.APPSECRET"
            android:value="408f92f2b9239e6b040f30fbbd399ee7" />
        <meta-data
            android:name="com.taobao.android.hotfix.RSASECRET"
            android:value="" />
        <!--ali hoxfix end-->

        <!--app 启动页面-->
        <activity
            android:name=".ui.introduce.LoadingActivity"
            android:configChanges="keyboardHidden|keyboard|orientation"
            android:screenOrientation="portrait"
            android:theme="@style/FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--introduce-->
        <activity
            android:name=".ui.introduce.IntroduceActivity"
            android:configChanges="keyboard|keyboardHidden|orientation"
            android:screenOrientation="portrait"
            android:theme="@style/FullScreen"></activity>
        <!--main-->
        <activity
            android:name=".ui.main.MainActivity"
            android:configChanges="keyboard|keyboardHidden|orientation"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>